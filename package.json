{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --single --cors --host",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "rebuild:index": "node scripts/rebuildIndex.js",
    "rebuild:servants": "node scripts/rebuildRepository.js",
    "update:data": "node scripts/updateDataset.js",
    "update:specify": "node scripts/UpdateSpecifyDataset.js --build",
    "update": "npm run update:data && npm run rebuild:index && npm run rebuild:servants && npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@zerodevx/svelte-toast": "^0.5.1",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "svelte-scrollto": "^0.2.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@octokit/core": "^3.5.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@smui/button": "^4.2.0",
    "algoliasearch": "^4.10.5",
    "bootstrap-icons": "^1.5.0",
    "chalk": "^4.1.2",
    "instantsearch.js": "^4.30.2",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-svelte-ssr": "^1.0.3",
    "sirv-cli": "^1.0.0",
    "svelte-algolia": "^0.2.7",
    "svelte-material-ui": "^4.2.0",
    "svelte-meta-tags": "^2.0.0",
    "svelte-routing": "^1.6.0",
    "unzipper": "^0.10.11",
    "vercel": "^23.1.2"
  }
}
